<?xml version="1.0" encoding="UTF-8"?>
<plugin_manifest xmlns="http://www.elgg.org/plugin_manifest/1.8">
	<name>Elgg-nodejs</name>
	<id>elgg-nodejs</id>
	<author>Emmanuel Salomon @ManUtopiK</author>
	<version>0.1</version>
	<category>integrations</category>
	<category>ggouv</category>
	<blurb>node.js framework for Elgg</blurb>
	<description>Provide a bi-directional realtime framework for Elgg to play with socket.io and easyRTC trough a node.js server</description>
	<website>https://github.com/ggouv/elgg-nodejs</website>
	<license>GNU General Public License version 2</license>
	<requires>
		<type>elgg_release</type>
		<version>1.9</version>
	</requires>
	<requires>
		<type>php_extension</type>
		<name>curl</name>
	</requires>
	<activate_on_install>true</activate_on_install>
</plugin_manifest>
